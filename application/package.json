{
  "name": "wretched-wind",
  "version": "1.0.0-beta.52",
  "description": "AstroWind: A free template using Astro 5.0 and Tailwind CSS. Astro starter theme.",
  "type": "module",
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >= 21.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --host",
    "prebuild": "node ./scripts/generate-downloadable-assets/index.mjs prepare",
    "build": "astro check && astro build",
    "postbuild": "node ./scripts/generate-downloadable-assets/index.mjs finalize",
    "preview": "astro preview",
    "astro": "ASTRO_TELEMETRY_DISABLED=1 astro",
    "humanize:check": "node ./scripts/humanize-content.mjs --check",
    "humanize:fix": "node ./scripts/humanize-content.mjs --write",
    "format": "prettier -w .",
    "lint": "npm run lint:prettier && npm run lint:astro && npm run humanize:check",
    "lint:astro": "astro check",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run lint:fix:prettier",
    "lint:ci": "npm run lint",
    "lint:fix:prettier": "prettier -w .",
    "test": "vitest run",
    "test:ci": "npm run test -- --reporter=default --reporter=junit --outputFile=junit.xml --coverage.enabled --coverage.reporter=lcov --coverage.reporter=text"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/roboto-mono": "^5.2.8",
    "@iconify-json/tabler": "^1.2.26",
    "archiver": "^7.0.1",
    "astro": "^5.17.1",
    "astro-embed": "^0.12.0",
    "astro-icon": "^1.1.5",
    "limax": "4.2.2",
    "lodash.merge": "^4.6.2",
    "marked": "^17.0.1",
    "puppeteer": "^24.37.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "unpic": "^4.2.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/markdown-remark": "^6.3.10",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/partytown": "^2.1.4",
    "@eslint/js": "^10.0.1",
    "@iconify-json/flat-color-icons": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/archiver": "^7.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/mdx": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "astro-compress": "2.3.9",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^10.0.0",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^17.3.0",
    "humanize-ai-lib": "^1.0.0",
    "js-yaml": "^4.1.1",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time": "^1.5.0",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "unist-util-visit": "^5.1.0",
    "vitest": "^4.0.17"
  }
}
