{
  "name": "wretched-wind",
  "version": "1.0.0-beta.52",
  "description": "AstroWind: A free template using Astro 5.0 and Tailwind CSS. Astro starter theme.",
  "type": "module",
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >= 21.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --host",
    "prebuild": "node ./scripts/generate-downloadable-assets/index.mjs prepare",
    "build": "astro check && astro build",
    "postbuild": "node ./scripts/generate-downloadable-assets/index.mjs finalize",
    "preview": "astro preview",
    "astro": "ASTRO_TELEMETRY_DISABLED=1 astro",
    "format": "prettier -w .",
    "lint": "npm run lint:prettier && npm run lint:astro",
    "lint:astro": "astro check",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run lint:fix:prettier",
    "lint:fix:prettier": "prettier -w .",
    "test:ci": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.13",
    "@astrojs/sitemap": "^3.6.0",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/roboto-mono": "^5.2.8",
    "archiver": "^7.0.1",
    "astro": "^5.15.5",
    "astro-embed": "^0.9.1",
    "astro-icon": "^1.1.5",
    "limax": "4.2.1",
    "lodash.merge": "^4.6.2",
    "puppeteer": "^24.30.0",
    "unpic": "^4.2.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/mdx": "^4.3.10",
    "@astrojs/partytown": "^2.1.4",
    "@eslint/js": "^9.39.1",
    "@iconify-json/flat-color-icons": "^1.2.3",
    "@iconify-json/tabler": "^1.2.23",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/archiver": "^7.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/mdx": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "astro-compress": "2.3.8",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^16.5.0",
    "js-yaml": "^4.1.0",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time": "^1.5.0",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "unist-util-visit": "^5.0.0"
  }
}
